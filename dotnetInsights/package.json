{
  "name": "dotnetInsights",
  "displayName": ".NET Insights",
  "publisher": "jashoo",
  "description": "Drill into .NET DLLs by viewing the IL and compiled code. Relies on ildasm and a public .NET JIT tool PMI.",
  "version": "0.1.1",
  "preview": true,
  "icon": "media/dep.png",
  "repository": {
    "url": "https://github.com/jashook/vscode-dotnet-insights"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCustomEditor:dotnetInsights.edit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "dotnetInsights.edit",
        "displayName": "Dotnet Insights Editor",
        "selector": [
          {
            "filenamePattern": "*.dll",
            "priority": "default"
          }
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dotnet-insights",
          "title": "Dotnet Insights",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "dotnet-insights": [
        {
          "id": "dotnetInsights",
          "name": "Dotnet Insights",
          "icon": "media/dep.svg",
          "contextualTitle": "Dotnet Insights"
        }
      ]
    },
    "commands": [
      {
        "command": "dotnetInsights.minOpts",
        "title": "Tier 0"
      },
      {
        "command": "dotnetInsights.tier1",
        "title": "Tier 1"
      },
      {
        "command": "dotnetInsights.diff",
        "title": "Diff"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "dotnetInsights.minOpts",
          "when": "view == dotnetInsights && viewItem == dependency"
        },
        {
          "command": "dotnetInsights.tier1",
          "when": "view == dotnetInsights && viewItem == dependency"
        },
        {
          "command": "dotnetInsights.diff",
          "when": "view == dotnetInsights && viewItem == dependency"
        }
      ]
    },
    "languages": [
      {
        "id": "ildasm",
        "aliases": [
          "ildasm",
          "ILDASM",
          "ilDasm",
          "ildasm"
        ],
        "extensions": [
          ".ildasm"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "asm",
        "aliases": [
          "asm",
          "ASM",
          "s",
          "S"
        ],
        "extensions": [
          ".asm"
        ],
        "configuration": "./asm-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ildasm",
        "scopeName": "source.ildasm",
        "path": "./syntaxes/ildasm.tmLanguage.json"
      },
      {
        "language": "asm",
        "scopeName": "source.asm",
        "path": "./syntaxes/asm.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/request": "^2.48.5",
    "@types/unzipper": "^0.10.3",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "unzipper": "^0.10.11",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@types/request": "^2.48.5",
    "@types/unzipper": "^0.10.3",
    "request": "^2.88.2",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2"
  }
}
