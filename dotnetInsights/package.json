{
  "name": "dotnetInsights",
  "displayName": "",
  "description": "Insights into you dotnet applications",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:ildasm.edit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "ildasm.edit",
        "displayName": ".NET IL",
        "selector": [
          {
            "filenamePattern": "*.dll",
            "priority": "default"
          }
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dotnet-insights",
          "title": "Dotnet Insights",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "dotnet-insights": [
        {
          "id": "dotnetInsights",
          "name": "Dotnet Insights",
          "icon": "media/dep.svg",
          "contextualTitle": "Dotnet Insights"
        }
      ]
    },
    "commands": [
      {
        "command": "dotnetInsights.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
